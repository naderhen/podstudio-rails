<div ng-controller="FeedSearchCtrl">
  <input ui-select2="select2Options" ng-model="select2model" style="width:200px" />
</div>

<%= form_tag("/home/fetch_feed", method: "get") do %>
  <%= label_tag(:url, "Feed URL:") %>
  <%= text_field_tag(:url) %>
  <%= submit_tag("Add") %>
<% end %>

<% @release_predictions.each do |day, predictions| %>
	<h1><%= day %></h1>
	<ul>
		<% predictions.each do |prediction| %>
			<li><%= prediction.podcast.name %>: <%= "#{prediction.percent * 100}%" %></li>
		<% end %>
	</ul>
<% end %>
